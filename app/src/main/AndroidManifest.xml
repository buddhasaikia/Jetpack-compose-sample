<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetpackComposeSample"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".LazyListWithStickyHeaderActivity"
            android:exported="true"
            android:label="@string/title_activity_lazy_list_with_sticky_header"
            android:theme="@style/Theme.JetpackComposeSample">

        </activity>
        <activity
            android:name=".LazyListActivity"
            android:label="@string/title_activity_lazy_list"
            android:theme="@style/Theme.JetpackComposeSample"/>
        <activity
            android:name=".ExpandableCardActivity"
            android:label="@string/title_activity_expandable_card"
            android:theme="@style/Theme.JetpackComposeSample"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.JetpackComposeSample" />
    </application>

</manifest>